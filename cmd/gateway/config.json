{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-gologging": {
      "level": "DEBUG",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "default"
    },
    "github_com/devopsfaith/krakend-logstash": {
      "enabled": false
    },
    "github_com/devopsfaith/krakend-metrics": {
      "collection_time": "60s",
      "proxy_disabled": false,
      "router_disabled": false,
      "backend_disabled": false,
      "endpoint_disabled": false,
      "listen_address": ":8090"
    },
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST"
      ]
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "port": 8080,
  "name": "GoChaos",
  "endpoints": [
    {
      "endpoint": "/user/{email}",
      "method": "GET",
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1,
      "querystring_params": [],
      "backend": [
        {
          "url_pattern": "/user/{email}",
          "encoding": "string",
          "sd": "static",
          "extra_config": {},
          "method": "GET",
          "host": [
            "http://localhost:8081"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/user",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "concurrent_calls": 1,
      "backend": [
        { 
          "host": [
            "http://localhost:8081"
          ],
          "url_pattern": "/user",
          "encoding": "no-op",
          "sd": "static",
          "extra_config": {},
          "method": "POST"
        }
      ]
    },
    {
      "endpoint": "/user/{email}",
      "method": "PUT",
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/user/{email}",
          "encoding": "string",
          "sd": "static",
          "extra_config": {},
          "method": "PUT",
          "host": [
            "http://localhost:8081"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/user/{email}",
      "method": "DELETE",
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/user/{email}",
          "encoding": "string",
          "sd": "static",
          "extra_config": {},
          "method": "DELETE",
          "host": [
            "http://localhost:8081"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}